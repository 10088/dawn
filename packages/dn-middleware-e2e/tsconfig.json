{
  "compilerOptions": {
    "moduleResolution": "node",
    "noImplicitAny": true,
    "allowJs": true,
    "module": "CommonJS",
    "target": "es5",
    "jsx": "react",
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": false,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "declaration": false,
    "lib": [
      "dom",
      "esnext"
    ]
  }
}