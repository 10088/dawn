{
  "name": "dawn",
  "description": "Dawn framework cli",
  "version": "2.0.0-alpha.0",
  "author": "Jeason @jeasonstudio",
  "bin": {
    "dn": "./bin/run",
    "dawn": "./bin/run"
  },
  "bugs": "https://github.com/alibaba/dawn/issues",
  "dependencies": {
    "@oclif/command": "^1.5.20",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^2.2.3",
    "confman": "^0.2.10",
    "del": "^5.1.0",
    "js-yaml": "^3.13.1",
    "mkdirp": "^1.0.4",
    "node-fetch": "^2.6.0",
    "pjson": "^1.0.9",
    "please-upgrade-node": "^3.2.0",
    "please-upgrade-npm": "^3.2.3",
    "signale": "^1.4.0",
    "stp": "0.0.4",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@types/chai": "^4.2.11",
    "@types/js-yaml": "^3.12.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.21",
    "@types/node-fetch": "^2.5.7",
    "@types/signale": "^1.4.1",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-dawn": "^2.0.0-beta.3",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "oclif": "^1.15.2",
    "prettier": "^2.0.5",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/alibaba/dawn",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dn",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "command_not_found": "./lib/hooks/command_not_found/runhook"
    }
  },
  "dawn": {
    "server": "https://alibaba.github.io/dawn",
    "registry": "https://registry.npm.taobao.org",
    "npm": "npm",
    "cache": 3600000,
    "middlewarePrefix": "dn-middleware",
    "templatePrefix": "dn-template"
  },
  "repository": "alibaba/dawn",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc.yml",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "types": "lib/index.d.ts"
}
